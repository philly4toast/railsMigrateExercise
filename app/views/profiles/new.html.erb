<h1>New Profile</h1>



<%= form_with scope: :profile, url: profiles_path, local: true do |form| %>
  <p>
    <%= form.label :first_name %><br>
    <%= form.text_field :first_name %>
  </p>
 
  <p>
    <%= form.label :last_name %><br>
    <%= form.text_field :last_name %>
  </p>
 
   <p>
    <%= form.label :title %><br>
    <%= form.text_field :title %>
  </p> 
 
  <p>
    <%= form.label :logline %><br>
    <%= form.text_area :logline %>
  </p>

  <p>
    <%= form.label :phone %><br>
    <%= form.text_field :phone %>
  </p>

  <p>
    <%= form.label :city %><br>
    <%= form.text_field :city %>
  </p>

  <p>
    <%= form.label :state %><br>
    <%= form.text_field :state %>
  </p>

<h2>Social Media</h2>

  <p>
    <%= form.label :facebook %>
    <%= form.text_field :facebook %>
  </p>

    <p>
    <%= form.label :instagram %>
    <%= form.text_field :instagram %>
  </p>

  <p>
    <%= form.label :linkedin %>
    <%= form.text_field :linkedin %>
  </p>

  <p>
    <%= form.label :other %>
    <%= form.text_field :other %>
  </p>



  <p>
    <%= form.submit %>
  </p>
<% end %>